    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.0.9",
    "@nestjs/platform-ws": "7.0.9",
    "@nestjs/typeorm": "7.0.0",
    "@nestjs/websockets": "7.0.9",
    "amqp-connection-manager": "3.2.0",
    "amqplib": "0.5.5",
    "bcryptjs": "2.4.3",
    "cache-manager": "3.2.1",
