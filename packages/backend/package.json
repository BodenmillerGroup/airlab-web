{
  "name": "@airlab/backend",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "pm2-runtime ecosystem.config.js",
    "lint": "eslint --ext .ts,.js .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:run": "ts-node ../../node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ../../node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@airlab/shared": "*",
    "@nestjs/common": "6.10.8",
    "@nestjs/core": "6.10.8",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/microservices": "6.10.8",
    "@nestjs/passport": "6.1.1",
    "@nestjs/platform-express": "6.10.8",
    "@nestjs/typeorm": "6.2.0",
    "@nestjsx/crud": "4.2.0",
    "@nestjsx/crud-typeorm": "4.2.0",
    "amqp-connection-manager": "3.1.0",
    "amqplib": "0.5.5",
    "bcryptjs": "2.4.3",
    "cache-manager": "2.10.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.0",
    "dotenv": "8.2.0",
    "helmet": "3.21.2",
    "mailgen": "2.0.10",
    "mysql": "2.17.1",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "7.14.0",
    "redis": "2.8.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3",
    "swagger-ui-express": "4.1.2",
    "typeorm": "0.2.21",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@google-cloud/storage": "4.1.3",
    "@nestjs/cli": "6.12.9",
    "@nestjs/schematics": "6.7.5",
    "@nestjs/testing": "6.10.8",
    "@types/bcryptjs": "2.4.2",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.2",
    "@types/mailgen": "2.0.2",
    "@types/node": "12.12.14",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "1.0.33",
    "@types/supertest": "2.0.8",
    "@types/uuid": "3.4.6",
    "dirty-json": "0.7.1",
    "mysql2": "2.0.2",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.4",
    "tsconfig-paths": "3.9.0"
  }
